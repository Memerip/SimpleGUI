-- Scripts:

local function GMVOO_fake_script() -- GUIclose.LocalScript 
	local script = Instance.new('LocalScript', GUIclose)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
end
coroutine.wrap(GMVOO_fake_script)()
local function HEBGWA_fake_script() -- IntroGui.LocalScript 
	local script = Instance.new('LocalScript', IntroGui)

	local plr = game.Players.LocalPlayer
	
	repeat wait() until plr.Character
	
	local gn = script.Parent.GameName.Value
	local gc = script.Parent.GameCreator.Value
	
	local rd = game:GetService("RunService")
	
	wait(1.8)
	
	local gctxt = Instance.new("TextLabel", script.Parent)
	gctxt.Text = "Coded by "..gc
	gctxt.BackgroundTransparency = 1
	gctxt.FontSize = "Size24"
	gctxt.Size = UDim2.new(1, 0, 0, 100)
	gctxt.Position = UDim2.new(0, 0, 0, -120)
	gctxt.TextColor3 = Color3.new(255/255, 255/255, 255/255)
	gctxt.TextStrokeTransparency = 0.4
	gctxt:TweenPosition(UDim2.new(0, 0, 0, 43))
	wait(0.9)
	local gntxt = Instance.new("TextLabel", script.Parent)
	gntxt.Text = gn
	gntxt.BackgroundTransparency = 1
	gntxt.FontSize = "Size36"
	gntxt.Size = UDim2.new(1, 0, 0, 100)
	gntxt.Position = UDim2.new(0, 0, 0, -120)
	gntxt.TextColor3 = Color3.new(255/255, 255/255, 255/255)
	gntxt.TextStrokeTransparency = 0.4
	gntxt:TweenPosition(UDim2.new(0, 0, 0, 10))
	
	wait(3.5)
	
	gntxt:TweenPosition(UDim2.new(0, 0, 0, -120))
	wait(0.9)
	gctxt:TweenPosition(UDim2.new(0, 0, 0, -120))
	wait(0.6)
	for i = 1, 10 do
		script.Parent.Background.BackgroundTransparency = script.Parent.Background.BackgroundTransparency + 0.1
		rd.RenderStepped:wait()
	end
end
coroutine.wrap(HEBGWA_fake_script)()
